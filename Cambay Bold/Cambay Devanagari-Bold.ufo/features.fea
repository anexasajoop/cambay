# Substitute with alternate candraBindu to avoid clash with iikar
sub @FullForms @iiMatraA candraBindu-deva' by candraBindu-deva.alt2;

# To make reph-ikar ligature
sub @iMatra' @FullForms [reph-deva] by @iMatraReph; 
sub @iMatraReph @FullForms' lookup removeReph [reph-deva]';
sub @iMatra' @Conjuncts [reph-deva] by @iMatraReph; 
sub @iMatraReph @Conjuncts' lookup removeReph [reph-deva]';

# To make reph-ikar-anusvara ligature
sub @iMatra' @FullForms [reph_anusvara-deva] by @iMatraRephAnusvara; 
sub @iMatraRephAnusvara @FullForms' lookup removeReph [reph-deva reph_anusvara-deva]';
sub @iMatra' @Conjuncts [reph_anusvara-deva] by @iMatraRephAnusvara; 
sub @iMatraRephAnusvara @Conjuncts' lookup removeReph [reph-deva reph_anusvara-deva]';

# To make reph-ikar-candraBindu ligature
sub @iMatra' @FullForms [reph_candraBindu-deva] by @iMatraRephCandraBindu; 
sub @iMatraRephCandraBindu @FullForms' lookup removeReph [reph-deva reph_candraBindu-deva]';
sub @iMatra' @Conjuncts [reph_candraBindu-deva] by @iMatraRephCandraBindu; 
sub @iMatraRephCandraBindu @Conjuncts' lookup removeReph [reph-deva reph_candraBindu-deva]';
} abvs;

